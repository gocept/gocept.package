[buildout]
extends =
    http://download.zope.org/zopetoolkit/index/1.1.3/ztk-versions.cfg
parts =
    doc
    test
develop = .
versions = versions
allow-picked-versions = false

[versions]
gocept.package = {{gocept_package_version}}
pkginfo = 0.8

[doc]
recipe = zc.recipe.egg
eggs = gocept.package [doc]

[test]
recipe = zc.recipe.testrunner
eggs = {{egg}} [test]
